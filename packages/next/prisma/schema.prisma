generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Project {
  id        Int     @id @default(autoincrement())
  shortcode String
  data      String
  version   Int     @default(1)
  
  @@unique([shortcode])
  @@unique([shortcode, version])
}

